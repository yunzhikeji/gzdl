<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yz.mapper.SiteMapperCustom" >

 <select id="querySiteBySiteCustom" parameterType="com.yz.po.SiteCustom" resultType="com.yz.po.Site">
 	SELECT * FROM site 
	<where>
			name=#{sname}
  		</where>
 </select>
   <select id="getSites" resultType="com.yz.po.Site"  >
    select * from site
  </select>
    <delete id="deleteByMarkid" parameterType="java.lang.String" >
    delete from site where markid = #{markid,jdbcType=VARCHAR}
  </delete>
   <select id="selectByMarkid" resultType="com.yz.po.Site" parameterType="java.lang.String" >
    select * from site where markid = #{markid,jdbcType=VARCHAR}
  </select>
</mapper>